Cell["So now we can write our expression for enclosed charge:", "Text",
 CellChangeTimes->{{3.64192481957327*^9, 3.64192483529327*^9}, 
   3.6500180708100414`*^9}],

Cell[BoxData[{
 RowBox[{"Clear", "[", "rho", "]"}], "\[IndentingNewLine]", 
 RowBox[{"qencslab", "=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "*", "z", "*", "area", "*", "rho"}], ",", 
       RowBox[{
        RowBox[{"Abs", "[", "z", "]"}], "<", 
        RowBox[{"W", "/", "2"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"W", "*", "area", "*", "rho"}], ",", 
       RowBox[{
        RowBox[{"Abs", "[", "z", "]"}], ">=", 
        RowBox[{"W", "/", "2"}]}]}], "}"}]}], "}"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.64192484074127*^9, 3.6419249439992704`*^9}, {
  3.64192499597227*^9, 3.64192499742027*^9}, {3.6419250294822702`*^9, 
  3.64192506943127*^9}, {3.64192510617327*^9, 3.64192511103627*^9}, {
  3.64192562130727*^9, 3.6419256216622705`*^9}}],

Cell["\<\
And our expression for flux (again assuming symmetry, with all flux going \
through the ends of our Gaussian Surface):\
\>", "Text",
 CellChangeTimes->{{3.64192519269427*^9, 3.64192522788427*^9}}],

Cell[BoxData[
 RowBox[{"fslab", "=", 
  RowBox[{"2", "*", "eslab", "*", "area", "*", "eps0"}]}]], "Input",
 CellChangeTimes->{{3.64192523657127*^9, 3.64192526363527*^9}, {
  3.6419256142122703`*^9, 3.64192561572427*^9}}],

Cell[BoxData[
 RowBox[{"Simplify", "[", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"fslab", "\[Equal]", "qencslab"}], ",", "eslab"}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.64192527334227*^9, 3.64192530697627*^9}, 
   3.64192538344327*^9, {3.6419256413702703`*^9, 3.64192564625727*^9}}],

Cell[TextData[{
 "We can see that inside the slab, we have a linear dependence on z, but \
outside the slab, our electric fields looks just like that of a sheet of \
charge with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "s"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=W",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "o"], TraditionalForm]]],
 ".  So let\[CloseCurlyQuote]s write this as an equation for a slab with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "o"], TraditionalForm]]],
 "=2",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", "o"], ",", " ", 
    RowBox[{
     RowBox[{"and", " ", "thickness", " ", "W"}], "=", 
     RowBox[{"1", "m"}]}]}], TraditionalForm]]],
 " :"
}], "Text",
 CellChangeTimes->{{3.64192542975027*^9, 3.6419255269922705`*^9}, {
  3.64192565655227*^9, 3.6419256880982704`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"rho", "=", 
   RowBox[{"2", "*", "eps0"}]}], ";", " ", 
  RowBox[{"W", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"eslab", "=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", 
         RowBox[{"z", "*", 
          RowBox[{"rho", "/", 
           RowBox[{"(", "eps0", ")"}]}]}]}], "}"}], ",", 
       RowBox[{
        RowBox[{"Abs", "[", "z", "]"}], "<", 
        RowBox[{"W", "/", "2"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", 
         RowBox[{
          RowBox[{"Sign", "[", "z", "]"}], "*", 
          RowBox[{"rho", "/", 
           RowBox[{"(", 
            RowBox[{"2", "*", "eps0"}], ")"}]}]}]}], "}"}], ",", 
       RowBox[{
        RowBox[{"Abs", "[", "z", "]"}], "\[GreaterEqual]", 
        RowBox[{"W", "/", "2"}]}]}], "}"}]}], "}"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.64192557583127*^9, 3.6419256018932705`*^9}, {
   3.64192569262227*^9, 3.64192578901727*^9}, {3.6419258523012705`*^9, 
   3.64192589937827*^9}, {3.64192597536627*^9, 3.64192597874027*^9}, 
   3.6419261041972704`*^9, {3.6419806134004526`*^9, 3.6419806297484527`*^9}, 
   3.650018169952955*^9}],

Cell[BoxData[
 RowBox[{"VectorPlot3D", "[", 
  RowBox[{"eslab", ",", " ", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", " ", 
   RowBox[{"{", 
    RowBox[{"y", ",", 
     RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", " ", 
   RowBox[{"{", 
    RowBox[{"z", ",", 
