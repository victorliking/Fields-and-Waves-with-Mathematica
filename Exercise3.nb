Cell["The charge enclosed is then:", "Text",
 CellChangeTimes->{{3.641895457223411*^9, 3.641895467295411*^9}}],

Cell[BoxData[
 RowBox[{"qsheet", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"area", "*", "rhos"}], ",", 
    RowBox[{"{", 
     RowBox[{"z", ",", 
      RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.641895473837811*^9, 3.641895486523611*^9}, 
   3.641895575815611*^9, {3.641895827718411*^9, 3.641895835348811*^9}, {
