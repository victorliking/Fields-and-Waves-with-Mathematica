Cell[CellGroupData[{

Cell["Electromotive Force from a Time-Varying Magnetic Field", "Subsection",
 CellChangeTimes->{{3.6459854084484754`*^9, 3.6459854232249527`*^9}}],

Cell[TextData[{
 "Let\[CloseCurlyQuote]s assume we have a magnetic field ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["B", "\[RightVector]"], "(", "t", ")"}], "=", 
    RowBox[{"10", 
     RowBox[{"Cos", "[", 
      RowBox[{"10", "\[Pi]t"}], "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"k", " "}], "^"], "[", "T", "]"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".  Calculate the electromotive force, as a function of time, generated \
around our loop.\nWe know we can use the same ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", 
    OverscriptBox["S", "\[RightVector]"]}], TraditionalForm]]],
 "=dsurf as we did above."
}], "Text",
 CellChangeTimes->{{3.6459854341610465`*^9, 3.6459854579234223`*^9}, {
  3.645985509415571*^9, 3.645985645778206*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"b1", "=", 
  RowBox[{"{", 
   RowBox[{"0", ",", "0", ",", 
    RowBox[{"10", "*", 
     RowBox[{"Cos", "[", 
      RowBox[{"10", "*", "Pi", "*", "t"}], "]"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.6459856627849064`*^9, 3.64598568742737*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", "0", ",", 
   RowBox[{"10", " ", 
    RowBox[{"Cos", "[", 
     RowBox[{"10", " ", "\[Pi]", " ", "t"}], "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{3.6525349994771147`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"emf1", "=", 
  RowBox[{"-", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"Dot", "[", 
        RowBox[{"b1", ",", "dsurf"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"theta", ",", "0", ",", 
         RowBox[{"2", "*", "Pi"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"rd", ",", "0", ",", "1"}], "}"}]}], "]"}], ",", "t"}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{{3.645985653488977*^9, 3.645985657969425*^9}, {
  3.645985694395067*^9, 3.6459857094605737`*^9}, {3.6459857605416813`*^9, 
  3.6459857671653433`*^9}, {3.6459858198476114`*^9, 3.645985844681094*^9}}],

Cell[BoxData[
 RowBox[{"100", " ", 
  SuperscriptBox["\[Pi]", "2"], " ", 
  RowBox[{"Sin", "[", 
   RowBox[{"10", " ", "\[Pi]", " ", "t"}], "]"}]}]], "Output",
 CellChangeTimes->{3.652535000789515*^9}]
}, Open  ]],

