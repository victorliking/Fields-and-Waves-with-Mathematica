Cell[TextData[{
 "For a surface parameterized by two parameters (rd and theta, in this case), \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     OverscriptBox["S", "\[RightVector]"]}], "=", 
    RowBox[{"(", 
     RowBox[{
      FractionBox[
       RowBox[{"\[DifferentialD]", "rsurf"}], 
       RowBox[{"\[DifferentialD]", "rd"}]], "\[Times]", 
      FractionBox[
       RowBox[{"\[DifferentialD]", "rsurf"}], 
       RowBox[{"\[DifferentialD]", "theta"}]]}], ")"}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.645984872660902*^9, 3.6459849980834427`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"dsurf", "=", 
  RowBox[{"Cross", "[", 
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{"rsurf", ",", "rd"}], "]"}], ",", 
    RowBox[{"D", "[", 
     RowBox[{"rsurf", ",", "theta"}], "]"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6459849806537*^9, 3.6459850304196763`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", "0", ",", 
   RowBox[{
    RowBox[{"rd", " ", 
     SuperscriptBox[
      RowBox[{"Cos", "[", "theta", "]"}], "2"]}], "+", 
    RowBox[{"rd", " ", 
     SuperscriptBox[
      RowBox[{"Sin", "[", "theta", "]"}], "2"]}]}]}], "}"}]], "Output",
 CellChangeTimes->{3.652534991214715*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"fsurf", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"rd", "*", 
     RowBox[{"Cos", "[", "theta", "]"}]}], ",", "0", ",", 
    RowBox[{"rd", "*", 
     RowBox[{"Cos", "[", "theta", "]"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.6521181135846405`*^9, 3.6521181387800407`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"rd", " ", 
    RowBox[{"Cos", "[", "theta", "]"}]}], ",", "0", ",", 
   RowBox[{"rd", " ", 
    RowBox[{"Cos", "[", "theta", "]"}]}]}], "}"}]], "Output",
 CellChangeTimes->{3.652534992434515*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"flux", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"Dot", "[", 
     RowBox[{"fsurf", ",", "dsurf"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"rd", ",", "0", ",", "1"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"theta", ",", "0", ",", 
      RowBox[{"2", "*", "Pi"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6521178850842404`*^9, 3.6521179587758403`*^9}, 
   3.65211799067924*^9, {3.6521181472060404`*^9, 3.6521181481118402`*^9}}],

Cell[BoxData["0"], "Output",
 CellChangeTimes->{3.6525349937917147`*^9}]
}, Open  ]],

Cell[TextData[{
 "Let\[CloseCurlyQuote]s make sure we are not crazy, and calculate the flux \
through our disk for a constant field  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["F", "\[RightVector]"], "(", 
     RowBox[{"x", ",", "y", ",", "z"}], ")"}], " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "=",
 Cell[BoxData[
  FormBox[
   OverscriptBox["k", "^"], TraditionalForm]]],
 ", which we know should give us \[Pi]",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.6459850639960337`*^9, 3.6459851388635197`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"Dot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "0", ",", "1"}], "}"}], ",", "dsurf"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"theta", ",", "0", ",", 
     RowBox[{"2", "*", "Pi"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"rd", ",", "0", ",", "1"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.6459851265582895`*^9, 3.6459851820488377`*^9}, {
  3.645985313509982*^9, 3.6459853404306746`*^9}}],

Cell[BoxData["\[Pi]"], "Output",
 CellChangeTimes->{3.6525349963083153`*^9}]
}, Open  ]],

