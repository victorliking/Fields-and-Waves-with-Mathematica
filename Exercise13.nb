Cell["\<\
Looks Good!
There are two ways to find the emf.  We can take the surface integral of BdS \
as a function of time, or we can calculate the line integral around the loop. \
 Let\[CloseCurlyQuote]s try the latter here: \
\>", "Text",
 CellChangeTimes->{{3.645987137475816*^9, 3.6459871401443424`*^9}, {
  3.646047965291023*^9, 3.6460480106080227`*^9}, {3.646048694748023*^9, 
  3.646048736481023*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"rmoving", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"2", "*", "t"}], "+", 
     RowBox[{"Cos", "[", 
      RowBox[{"2", "*", "Pi", "*", "theta"}], "]"}]}], ",", 
    RowBox[{"Sin", "[", 
     RowBox[{"2", "*", "Pi", "*", "theta"}], "]"}], ",", "0"}], 
   "}"}]}]], "Input",
 CellChangeTimes->{{3.6460480315110226`*^9, 3.646048036455023*^9}, {
  3.646048749418023*^9, 3.6460487799190226`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"2", " ", "t"}], "+", 
    RowBox[{"Cos", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}], ",", 
   RowBox[{"Sin", "[", 
    RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}], ",", "0"}], 
  "}"}]], "Output",
 CellChangeTimes->{3.652535014495715*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"drmoving", "=", 
  RowBox[{"D", "[", 
   RowBox[{"rmoving", ",", "theta"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.646048066573023*^9, 3.6460480693970227`*^9}, {
  3.6460488025180225`*^9, 3.6460488122060227`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"-", "2"}], " ", "\[Pi]", " ", 
    RowBox[{"Sin", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}], ",", 
   RowBox[{"2", " ", "\[Pi]", " ", 
    RowBox[{"Cos", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}], ",", "0"}], 
  "}"}]], "Output",
 CellChangeTimes->{3.6525350161805153`*^9}]
}, Open  ]],

Cell["\<\
Our B, in parametric terms, along the path of the circle, is:\
\>", "Text",
 CellChangeTimes->{{3.6460480847870226`*^9, 3.6460481150090227`*^9}, 
   3.646048821668023*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"bwirecylt", "=", 
  RowBox[{"{", 
   RowBox[{"0", ",", "0", ",", 
    FractionBox[
     RowBox[{
      RowBox[{"-", "2.000577634665124`*^-7"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "*", "t"}], "+", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", "*", "Pi", "*", "theta"}], "]"}], "+", "2"}], ")"}]}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "*", "t"}], "+", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", "*", "Pi", "*", "theta"}], "]"}], "+", "2"}], ")"}], 
      "2"]]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.646048100276023*^9, 3.6460481345700226`*^9}, {
  3.646048835965023*^9, 3.6460488475880227`*^9}, {3.646048920783023*^9, 
  3.646048933903023*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", "0", ",", 
   RowBox[{"-", 
    FractionBox["2.000577634665124`*^-7", 
     RowBox[{"2", "+", 
      RowBox[{"2", " ", "t"}], "+", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}]]}]}], 
  "}"}]], "Output",
 CellChangeTimes->{3.652535017475315*^9}]
}, Open  ]],

Cell["The cross product of of v and B :", "Text",
 CellChangeTimes->{{3.646048147503023*^9, 3.6460481563340225`*^9}, {
  3.646048940468023*^9, 3.6460489484120226`*^9}, {3.6521182633936405`*^9, 
  3.6521182646104403`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"vcrossB", "=", 
  RowBox[{"Simplify", "[", 
   RowBox[{"Cross", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"2", ",", "0", ",", "0"}], "}"}], ",", "bwirecylt"}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.646048160422023*^9, 3.646048183278023*^9}, {
  3.646048290711023*^9, 3.646048295934023*^9}, {3.6460489522210226`*^9, 
  3.6460489720510225`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0.`", ",", 
   FractionBox["4.001155269330248`*^-7", 
    RowBox[{"2", "+", 
     RowBox[{"2", " ", "t"}], "+", 
     RowBox[{"Cos", "[", 
      RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}]], ",", "0"}], 
  "}"}]], "Output",
 CellChangeTimes->{3.652535019146515*^9}]
}, Open  ]],

Cell["We take the dot product of v x B with dr:", "Text",
 CellChangeTimes->{{3.646048196564023*^9, 3.6460482015150228`*^9}, {
  3.646048982521023*^9, 3.646049002208023*^9}, {3.646049081619023*^9, 
  3.646049116017023*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"vcrossBdr", "=", 
  RowBox[{"Dot", "[", 
   RowBox[{"vcrossB", ",", "drmoving"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.646048210413023*^9, 3.6460482395220227`*^9}, {
  3.646049005033023*^9, 3.6460490565020227`*^9}, {3.646049124258023*^9, 
  3.646049137416023*^9}}],

Cell[BoxData[
 RowBox[{"0.`", "\[VeryThinSpace]", "+", 
  FractionBox[
   RowBox[{"2.5139999999999996`*^-6", " ", 
    RowBox[{"Cos", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}], 
   RowBox[{"2", "+", 
    RowBox[{"2", " ", "t"}], "+", 
    RowBox[{"Cos", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}]]}]], "Output",
 CellChangeTimes->{3.652535020737715*^9}]
}, Open  ]],

Cell["\<\
And then the line integral...basically integrate over theta from 0 to 1\
\>", "Text",
 CellChangeTimes->{{3.6460491573340225`*^9, 3.646049171885023*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"emf2", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    FractionBox[
     RowBox[{"2.514", "*", 
      RowBox[{"10", "^", 
       RowBox[{"-", "6"}]}], " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}], 
     RowBox[{"2", "+", 
      RowBox[{"2", " ", "t"}], "+", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "\[Pi]", " ", "theta"}], "]"}]}]], ",", 
    RowBox[{"{", 
     RowBox[{"theta", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.6460491182590227`*^9, 3.646049118260023*^9}, {
  3.6460491765980225`*^9, 3.646049221212023*^9}}],

Cell[BoxData[
 RowBox[{"2.5139999999999996`*^-6", " ", 
  RowBox[{"(", 
   RowBox[{"1", "-", 
    FractionBox[
     RowBox[{"2", " ", 
      RowBox[{"(", 
       RowBox[{"1", "+", "t"}], ")"}], " ", 
      SqrtBox[
       FractionBox[
        RowBox[{"1", "+", 
         RowBox[{"2", " ", "t"}]}], 
        RowBox[{"3", "+", 
         RowBox[{"2", " ", "t"}]}]]]}], 
     RowBox[{"1", "+", 
      RowBox[{"2", " ", "t"}]}]]}], ")"}]}]], "Output",
 CellChangeTimes->{3.6525350286995153`*^9, 3.652535078670115*^9}]
}, Open  ]],

