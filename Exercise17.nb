Cell[TextData[{
 "which means that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      OverscriptBox["B", "\[RightVector]"]}], 
     RowBox[{"\[PartialD]", "t"}]], "=", "0"}], TraditionalForm]]],
 ", so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["B", "\[RightVector]"], "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["B", "x"], ",", 
       SubscriptBox["B", "y"], ",", 
       SubscriptBox["B", "z"]}], "}"}], "=", "constant"}]}], 
   TraditionalForm]]],
 " in time.  But what about \[Del]\[Cross]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["H", "\[RightVector]"], "=", 
    RowBox[{
     OverscriptBox["J", "\[RightVector]"], "+", 
     FractionBox[
      RowBox[{"\[PartialD]", 
       OverscriptBox["D", "\[RightVector]"]}], 
      RowBox[{"\[PartialD]", "t"}]]}]}], TraditionalForm]]]
}], "Text",
