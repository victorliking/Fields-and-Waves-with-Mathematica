Cell["\<\
We can find the potential across the junction, from -w1 to w2, by taking the \
line integral \
\>", "Text",
 CellChangeTimes->{{3.6449198382942877`*^9, 3.64491990441166*^9}, {
  3.6449241633054485`*^9, 3.644924211480123*^9}, {3.645708918213208*^9, 
  3.645708952266018*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"vpn", "[", 
   RowBox[{"w1_", ",", "rho1_", ",", "w2_", ",", "rho2_"}], "]"}], ":=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"etotal", "[", 
      RowBox[{"w1", ",", "rho1", ",", "w2", ",", "rho2"}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"z", ",", 
      RowBox[{"-", "w1"}], ",", "w2"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.64405913401083*^9, 3.6440591597954082`*^9}, {
   3.6449199139939375`*^9, 3.6449199497434793`*^9}, {3.644924218330391*^9, 
   3.644924228628193*^9}, 3.6449247436032867`*^9, {3.645708958340232*^9, 
   3.645709012961897*^9}, {3.645709393790976*^9, 3.645709422072804*^9}}],

